{
  "name": "sriram-first-bower-package",
  "description": "testing how to create bower package",
  "version": "0.0.4",
  "main": [
    "app/scripts/app.js"
  ],
  "dependencies": {
    "angular": "1.5.8",
    "bootstrap": "^3.2.0",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-cookies": "1.5.8",
    "angular-messages": "1.5.8",
    "angular-resource": "1.5.8",
    "angular-route": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-touch": "1.5.8"
  },
  "devDependencies": {
    "angular-mocks": "1.5.8"
  },
  "authors": [
    "sriram rudraraju"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "appPath": "app",
  "moduleName": "bowerPackageApp",
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    }
  }
}
